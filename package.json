{
  "name": "comps-01",
  "version": "0.0.1",
  "description": "comps_01",
  "keywords": "es6, modules, stdlib, util",
  "homepage": "",
  "bugs": "",
  "repository": "ajrothwell/comps_01",
  "license": "MIT",
  "main": "dist/comps-01.umd.js",
  "module": "dist/comps-01.esm.js",
  "unpkg": "dist/comps-01.min.js",
  "browser": {
    "./sfc": "src/comps-01.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/comps-01.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/comps-01.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/comps-01.min.js"
  },
  "devDependencies": {
    "rollup": "^1.1.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-vue": "^4.6.1",
    "vue": "^2.5.22",
    "vue-template-compiler": "^2.5.22"
  }
}
